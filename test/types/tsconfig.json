/*
	This file is used to test the embedded declarations in TS / strict mode.
*/

{
    "include": ["./*.ts"],
    "compilerOptions": {
        "module": "ESNext",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "target": "es6",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true
    }
}
